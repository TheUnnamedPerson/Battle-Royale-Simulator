                                                        <TextBox x:Name="Char3StaLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                    </StackPanel>

                                                </Grid>
                                            </Viewbox>
                                            <Viewbox HorizontalAlignment="Stretch" VerticalAlignment="Center">
                                                <Grid MinWidth="375" Height="50" Grid.Row="1" Grid.Column="0" HorizontalAlignment="Center" >
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="1*"/>
                                                        <ColumnDefinition Width="2*"/>
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock  Width="Auto" Text="4:" Foreground="White" Grid.Column="0" FontSize="24" FontFamily="Futura Md BT" HorizontalTextAlignment="Left" VerticalAlignment="Center" />
                                                    <StackPanel Grid.Column="1" Orientation="Horizontal" Margin="10,5,10,0">
                                                        <TextBox x:Name="Char4ConsLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char4LukLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char4ChaosLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char4SanLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char4IntLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
