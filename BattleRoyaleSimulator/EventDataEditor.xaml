<Page
    x:Class="BattleRoyaleSimulator.EventDataEditor"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:BattleRoyaleSimulator"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:src="using:MonoTorrent.GUI.Controls"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}" FocusVisualPrimaryBrush="{x:Null}" FocusVisualSecondaryBrush="{x:Null}" Foreground="{x:Null}">

    <Grid Background="#FF121212" FocusVisualSecondaryBrush="{x:Null}" FocusVisualPrimaryBrush="{x:Null}">
        <Grid x:Name="MainGrid" Background="#FF121212" FocusVisualSecondaryBrush="{x:Null}" FocusVisualPrimaryBrush="{x:Null}">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="1*"/>
            </Grid.ColumnDefinitions>

            <Grid FocusVisualPrimaryBrush="{x:Null}" FocusVisualSecondaryBrush="{x:Null}" HorizontalAlignment="Left">


                <Viewbox VerticalAlignment="Stretch" HorizontalAlignment="Left">

                    <ScrollViewer Height="1010" MinWidth="588" MaxWidth="800">
                        <StackPanel x:Name="EditorPanel" Height="Auto" MinWidth="588" MaxWidth="800" Background="#FF282828" FocusVisualSecondaryBrush="{x:Null}" FocusVisualPrimaryBrush="{x:Null}" Grid.Column="0" Margin="15,15,15,15" HorizontalAlignment="Left">

                            <Grid Height="60" Grid.Row="1" Grid.Column="0" Margin="20,20,0,0" FlowDirection="RightToLeft">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="80"/>
                                    <ColumnDefinition Width="1*"/>
                                </Grid.ColumnDefinitions>
                                <Button Content="+" FontSize="32" Margin="0,0,0,0" Height="60" Width="60" HorizontalAlignment="Right" Grid.Column="0" FontWeight="Bold" Click="AddButton_Click" />
                                <TextBlock  Height="60" Width="Auto" Foreground="White" FontSize="48" FontFamily="Futura Md BT" Margin="20,0,80,0" Grid.Column="1" Grid.ColumnSpan="1" Text="{Binding ElementName=nameInput, Path=Text}" TextAlignment="Center" />
                            </Grid>

                            <StackPanel MinWidth="550" Grid.Row="1" Grid.Column="0" Margin="20,20,20,0" HorizontalAlignment="Center" >
                                <TextBlock  Width="Auto" Text="Name:" Foreground="White" FontSize="24" FontFamily="Futura Md BT" HorizontalTextAlignment="Center" VerticalAlignment="Center" />
                                <TextBox x:Name="nameInput" Text="Event" Height="40" Width="200" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="10,10,0,0" FontFamily="Futura Md BT" FontSize="24"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Grid.Row="1" Grid.Column="0" Margin="20,20,20,0" HorizontalAlignment="Center" >
                                <StackPanel Margin="0,0,10,0">
                                    <TextBlock  Width="Auto" Text="ID:" Foreground="White" FontSize="24" FontFamily="Futura Md BT" HorizontalTextAlignment="Center" VerticalAlignment="Center" />
                                    <TextBox x:Name="IdInput" Height="40" Width="200" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="10,10,0,0" FontFamily="Futura Md BT" FontSize="24"/>
                                </StackPanel>
                                <StackPanel Margin="0,0,10,0">
                                    <TextBlock  Width="Auto" Text="Weight:" Foreground="White" FontSize="24" FontFamily="Futura Md BT" HorizontalTextAlignment="Center" VerticalAlignment="Center" />
                                    <TextBox x:Name="WeightInput" Height="40" Width="200" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="10,10,0,0" FontFamily="Futura Md BT" FontSize="24"/>
                                </StackPanel>
                                <StackPanel>
                                    <TextBlock  Width="Auto" Text="Namespace:" Foreground="White" FontSize="24" FontFamily="Futura Md BT" HorizontalTextAlignment="Center" VerticalAlignment="Center" />
                                    <TextBox x:Name="NamespaceInput" Height="40" Width="200" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="10,10,0,0" FontFamily="Futura Md BT" FontSize="24"/>
                                </StackPanel>
                            </StackPanel>

                            <StackPanel MinWidth="550" Grid.Row="1" Grid.Column="0" Margin="20,20,20,0" HorizontalAlignment="Center" >
                                <TextBlock Width="Auto" Text="Event Text" Foreground="White" FontSize="24" FontFamily="Futura Md BT" HorizontalAlignment="Center" HorizontalTextAlignment="Center" VerticalAlignment="Center" Margin="20,0,0,0" />
                                <TextBox x:Name="TextInput" Height="120" Width="500" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="10,10,0,0" InputScope="Number" FontSize="24" FontFamily="Futura Md BT" TextWrapping="Wrap"/>
                            </StackPanel>
                            <StackPanel MinWidth="550" Grid.Row="1" Grid.Column="0" Margin="20,20,20,0" Orientation="Horizontal" HorizontalAlignment="Center" >
                                <StackPanel Orientation="Horizontal" Margin="20,0,0,0" HorizontalAlignment="Center" >
                                    <TextBlock Width="Auto" Text="Special Event" Foreground="White" FontSize="24" FontFamily="Futura Md BT" HorizontalAlignment="Center" HorizontalTextAlignment="Center" VerticalAlignment="Center" Margin="20,0,0,0" />
                                    <ToggleSwitch x:Name="SpecialInput" Margin="10,10,0,0" Height="40" Width="80" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="20,0,0,0" HorizontalAlignment="Center" >
                                    <TextBlock Width="Auto" Text="Absurd Event" Foreground="White" FontSize="24" FontFamily="Futura Md BT" HorizontalAlignment="Center" HorizontalTextAlignment="Center" VerticalAlignment="Center" Margin="20,0,0,0" />
                                    <ToggleSwitch x:Name="AbsurdInput" Margin="10,10,0,0" Height="40" Width="80" />
                                </StackPanel>
                            </StackPanel>

                            <Viewbox HorizontalAlignment="Stretch" VerticalAlignment="Center" Margin="20,10,20,0" >
                                <Grid MinWidth="550" Height="40" Grid.Row="1" Grid.Column="0" HorizontalAlignment="Center" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="1*"/>
                                        <ColumnDefinition Width="2*"/>
                                        <ColumnDefinition Width="1*"/>
                                        <ColumnDefinition Width="2*"/>
                                        <ColumnDefinition Width="1*"/>
                                        <ColumnDefinition Width="2*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock  Width="Auto" Text="Killers:" Foreground="White" Grid.Column="0" FontSize="24" FontFamily="Futura Md BT" HorizontalTextAlignment="Left" VerticalAlignment="Center" />
                                    <src:MultiSelectComboBox x:Name="KillersList" Width="100" Grid.Column="1" VerticalAlignment="Center" Margin="10,10,10,0"
                            ItemsSource="{Binding Values}"
                            SelectedItems="{Binding SelectedValues, Mode=TwoWay}"
                            NoSelectionText="{Binding EmptySelectionString}"
                            MultipleSelectionTextFormat="{Binding MultipleSelectedFormatString}"                                                              
                            PopupHeight="500" PopupWidth="200"
                            HorizontalAlignment="Stretch" FontSize="14" MinHeight="26" Height="26">
                                        <src:MultiSelectComboBox.ItemTemplate>
                                            <DataTemplate>
                                                <TextBlock Text="{Binding}"/>
                                            </DataTemplate>
                                        </src:MultiSelectComboBox.ItemTemplate>
                                    </src:MultiSelectComboBox>
                                    <TextBlock  Width="Auto" Text="Killed:" Foreground="White" Grid.Column="2" FontSize="24" FontFamily="Futura Md BT" HorizontalTextAlignment="Left" VerticalAlignment="Center" />
                                    <src:MultiSelectComboBox x:Name="KilledList" Width="100" Grid.Column="3" VerticalAlignment="Center" Margin="10,10,10,0"
                            ItemsSource="{Binding Values}"
                            SelectedItems="{Binding SelectedValues, Mode=TwoWay}"
                            NoSelectionText="{Binding EmptySelectionString}"
                            MultipleSelectionTextFormat="{Binding MultipleSelectedFormatString}"                                                              
                            PopupHeight="500" PopupWidth="200"
                            HorizontalAlignment="Stretch" FontSize="14" MinHeight="26" Height="26">
                                        <src:MultiSelectComboBox.ItemTemplate>
                                            <DataTemplate>
                                                <TextBlock Text="{Binding}"/>
                                            </DataTemplate>
                                        </src:MultiSelectComboBox.ItemTemplate>
                                    </src:MultiSelectComboBox>
                                    <TextBlock  Width="Auto" Text="Characters:" Foreground="White" Grid.Column="4" FontSize="24" FontFamily="Futura Md BT" HorizontalTextAlignment="Left" VerticalAlignment="Center" />
                                    <Slider x:Name="CharSlider" Height="40" Width="100" HorizontalAlignment="Center" Grid.Column="5" VerticalAlignment="Center" Margin="10,10,10,0" Style="{ThemeResource CustomSlideStyle}" StepFrequency="1" Maximum="10" Minimum="1" Value="1" />
                                </Grid>
                            </Viewbox>

                            <Viewbox HorizontalAlignment="Stretch" VerticalAlignment="Center" Margin="20,10,20,0" >
                                <Grid MinWidth="550" Height="40" Grid.Row="1" Grid.Column="0" HorizontalAlignment="Center" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="1*"/>
                                        <ColumnDefinition Width="2*"/>
                                        <ColumnDefinition Width="1*"/>
                                        <ColumnDefinition Width="2*"/>
                                        <ColumnDefinition Width="1*"/>
                                        <ColumnDefinition Width="2*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock  Width="Auto" Text="Chimps:" Foreground="White" Grid.Column="0" FontSize="24" FontFamily="Futura Md BT" HorizontalTextAlignment="Left" VerticalAlignment="Center" />
                                    <Slider x:Name="ChimpSlider" Height="40" Width="100" HorizontalAlignment="Center" Grid.Column="1" VerticalAlignment="Center" Margin="10,10,10,0" Style="{ThemeResource CustomSlideStyle}" StepFrequency="1" Maximum="10" Minimum="0" />
                                    <TextBlock  Width="Auto" Text="Bees:" Foreground="White" Grid.Column="2" FontSize="24" FontFamily="Futura Md BT" HorizontalTextAlignment="Left" VerticalAlignment="Center" />
                                    <Slider x:Name="BeeSlider" Height="40" Width="100" HorizontalAlignment="Center" Grid.Column="3" VerticalAlignment="Center" Margin="10,10,10,0" Style="{ThemeResource CustomSlideStyle}" StepFrequency="1" Maximum="10" Minimum="0" />
                                    <TextBlock  Width="Auto" Text="Rats:" Foreground="White" Grid.Column="4" FontSize="24" FontFamily="Futura Md BT" HorizontalTextAlignment="Left" VerticalAlignment="Center" />
                                    <Slider x:Name="RatSlider" Height="40" Width="100" HorizontalAlignment="Center" Grid.Column="5" VerticalAlignment="Center" Margin="10,10,10,0" Style="{ThemeResource CustomSlideStyle}" StepFrequency="1" Maximum="10" Minimum="0" />
                                </Grid>
                            </Viewbox>

                            <Viewbox HorizontalAlignment="Stretch" VerticalAlignment="Center" Margin="20,10,20,0" >
                                <Grid MinWidth="550" Height="40" Grid.Row="1" Grid.Column="0" HorizontalAlignment="Center" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="1*"/>
                                        <ColumnDefinition Width="2*"/>
                                        <ColumnDefinition Width="1*"/>
                                        <ColumnDefinition Width="2*"/>
                                        <ColumnDefinition Width="1*"/>
                                        <ColumnDefinition Width="2*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock  Width="Auto" Text="Revives:" Foreground="White" Grid.Column="0" FontSize="24" FontFamily="Futura Md BT" HorizontalTextAlignment="Left" VerticalAlignment="Center" />
                                    <src:MultiSelectComboBox x:Name="ReviveList" Width="100" Grid.Column="1" VerticalAlignment="Center" Margin="10,10,10,0"
                            ItemsSource="{Binding Values}"
                            SelectedItems="{Binding SelectedValues, Mode=TwoWay}"
                            NoSelectionText="{Binding EmptySelectionString}"
                            MultipleSelectionTextFormat="{Binding MultipleSelectedFormatString}"                                                              
                            PopupHeight="500" PopupWidth="200"
                            HorizontalAlignment="Stretch" FontSize="14" MinHeight="26" Height="26">
                                        <src:MultiSelectComboBox.ItemTemplate>
                                            <DataTemplate>
                                                <TextBlock Text="{Binding}"/>
                                            </DataTemplate>
                                        </src:MultiSelectComboBox.ItemTemplate>
                                    </src:MultiSelectComboBox>
                                    <TextBlock  Width="Auto" Text="Clones:" Foreground="White" Grid.Column="2" FontSize="24" FontFamily="Futura Md BT" HorizontalTextAlignment="Left" VerticalAlignment="Center" />
