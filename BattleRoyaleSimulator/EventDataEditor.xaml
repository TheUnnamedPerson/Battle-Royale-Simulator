                            ItemsSource="{Binding Values}"
                            SelectedItems="{Binding SelectedValues, Mode=TwoWay}"
                            NoSelectionText="{Binding EmptySelectionString}"
                            MultipleSelectionTextFormat="{Binding MultipleSelectedFormatString}"                                                              
                            PopupHeight="500" PopupWidth="200"
                            HorizontalAlignment="Stretch" FontSize="14" MinHeight="26" Height="26">
                                        <src:MultiSelectComboBox.ItemTemplate>
                                            <DataTemplate>
                                                <TextBlock Text="{Binding}"/>
                                            </DataTemplate>
                                        </src:MultiSelectComboBox.ItemTemplate>
                                    </src:MultiSelectComboBox>
                                    <TextBlock  Width="Auto" Text="Zombie:" Foreground="White" Grid.Column="4" FontSize="24" FontFamily="Futura Md BT" HorizontalTextAlignment="Left" VerticalAlignment="Center" />
                                    <src:MultiSelectComboBox x:Name="ZombieList" Width="100" Grid.Column="5" VerticalAlignment="Center" Margin="10,10,10,0"
                            ItemsSource="{Binding Values}"
                            SelectedItems="{Binding SelectedValues, Mode=TwoWay}"
                            NoSelectionText="{Binding EmptySelectionString}"
                            MultipleSelectionTextFormat="{Binding MultipleSelectedFormatString}"                                                              
                            PopupHeight="500" PopupWidth="200"
                            HorizontalAlignment="Stretch" FontSize="14" MinHeight="26" Height="26">
                                        <src:MultiSelectComboBox.ItemTemplate>
                                            <DataTemplate>
                                                <TextBlock Text="{Binding}"/>
                                            </DataTemplate>
                                        </src:MultiSelectComboBox.ItemTemplate>
                                    </src:MultiSelectComboBox>
                                </Grid>
                            </Viewbox>

                            <Viewbox HorizontalAlignment="Stretch" VerticalAlignment="Center" Margin="20,10,20,0" >
                                <Grid x:Name="StatsLowerGrid" MinWidth="550" Height="350" Grid.Row="1" Grid.Column="0" HorizontalAlignment="Center">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <ListView x:Name="StatsLowerList" Height="350" Width="525" HorizontalAlignment="Center" Grid.Column="1" VerticalAlignment="Top" Margin="0,5,0,0" SelectionMode="None" ScrollViewer.VerticalScrollMode="Disabled">
                                        <StackPanel Width="500" Height="Auto" Margin="0,0,0,0" HorizontalAlignment="Left">
                                            <Button x:Name="StatLowerButton" Content="Stat Minimums" FontSize="24" HorizontalContentAlignment="Center" FontFamily="Futura Md BT" Width="500" Height="40" Margin="0,0,0,5" Click="StatLowerButton_Click" />
                                            <Viewbox HorizontalAlignment="Stretch" VerticalAlignment="Center">
                                                <Grid MinWidth="375" Height="50" Grid.Row="1" Grid.Column="0" HorizontalAlignment="Center" >
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="1*"/>
                                                        <ColumnDefinition Width="2*"/>
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock  Width="Auto" Text="1:" Foreground="White" Grid.Column="0" FontSize="24" FontFamily="Futura Md BT" HorizontalTextAlignment="Left" VerticalAlignment="Center" />
                                                    <StackPanel Grid.Column="1" Orientation="Horizontal" Margin="10,5,10,0">
                                                        <TextBox x:Name="Char1ConsLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char1LukLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char1ChaosLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char1SanLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char1IntLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char1MarLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char1ChaLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char1DilLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char1EmpLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char1ResLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char1DetLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char1StaLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                    </StackPanel>

                                                </Grid>
                                            </Viewbox>
                                            <Viewbox HorizontalAlignment="Stretch" VerticalAlignment="Center">
                                                <Grid MinWidth="375" Height="50" Grid.Row="1" Grid.Column="0" HorizontalAlignment="Center" >
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="1*"/>
                                                        <ColumnDefinition Width="2*"/>
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock  Width="Auto" Text="2:" Foreground="White" Grid.Column="0" FontSize="24" FontFamily="Futura Md BT" HorizontalTextAlignment="Left" VerticalAlignment="Center" />
                                                    <StackPanel Grid.Column="1" Orientation="Horizontal" Margin="10,5,10,0">
                                                        <TextBox x:Name="Char2ConsLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char2LukLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char2ChaosLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char2SanLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char2IntLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char2MarLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char2ChaLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char2DilLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char2EmpLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char2ResLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char2DetLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                        <TextBox x:Name="Char2StaLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
                                                    </StackPanel>

                                                </Grid>
                                            </Viewbox>
                                            <Viewbox HorizontalAlignment="Stretch" VerticalAlignment="Center">
                                                <Grid MinWidth="375" Height="50" Grid.Row="1" Grid.Column="0" HorizontalAlignment="Center" >
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="1*"/>
                                                        <ColumnDefinition Width="2*"/>
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock  Width="Auto" Text="3:" Foreground="White" Grid.Column="0" FontSize="24" FontFamily="Futura Md BT" HorizontalTextAlignment="Left" VerticalAlignment="Center" />
                                                    <StackPanel Grid.Column="1" Orientation="Horizontal" Margin="10,5,10,0">
                                                        <TextBox x:Name="Char3ConsLower" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,5,0"/>
